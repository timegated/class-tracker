{
  "name": "class-tracker",
  "version": "1.0.0",
  "description": "Web app for tracking items in game.",
  "main": "server.js",
  "engines": {
    "node": "12.16.2",
    "yarn": "1.22.4"
  },
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "dev": "SET NODE_ENV=development & concurrently \"yarn run server\" \"yarn run client\"",
    "server": "SET NODE_ENV=development & nodemon dist/server.js",
    "test":"SET NODE_ENV=testing jest --forceExit --detectOpenHandles --silent",
    "test-models": "yarn test -t model",
    "test-auth": "yarn test -t Authentication:",
    "client": "npm start --prefix client",
    "clientinstall": "yarn install --prefix client",
    "restart": "rimraf dist && yarn build && yarn start",
    "start": "SET NODE_ENV=production & node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timegated/class-tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timegated/class-tracker/issues"
  },
  "homepage": "https://github.com/timegated/class-tracker#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "mock-req-res": "^1.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "wow-classic-items": "^0.8.1"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "textEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test-db-setup-js",
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  }
}
